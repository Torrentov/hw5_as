{
  "name": "one_batch_test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000
  },
  "augmentations": {
    "wave": [],
    "spectrogram": []
  },
  "arch": {
    "type": "RawNet2",
    "args": {
      "sinc_out_channels": 128,
      "sinc_kernel_size": 129,
      "resblocks_channels": [
        [128, 128],
        [128, 128],
        [128, 512],
        [512, 512],
        [512, 512],
        [512, 512]
      ],
      "gru_size": 1024,
      "gru_num_layers": 3,
      "fc_size": 1024,
      "num_classes": 2
    }
  },
  "data": {
    "train": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "part": "train",
            "limit": 10,
            "audio_dir": "data/LA/LA"
          }
        }
      ]
    },
    "val": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "part": "dev",
            "limit": 10,
            "audio_dir": "data/LA/LA"
          }
        }
      ]
    },
    "test": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "part": "eval",
            "limit": 10,
            "audio_dir": "data/LA/LA"
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 2e-4,
      "betas": [0.8, 0.99],
      "weight_decay": 0.01
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "args": {}
  },
  "metrics": [],
  "lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
      "gamma": 0.99
    }
  },
  "trainer": {
    "epochs": 100,
    "save_dir": "saved/",
    "save_period": 10,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "as_project",
    "len_epoch": 10,
    "grad_norm_clip": 10
  }
}

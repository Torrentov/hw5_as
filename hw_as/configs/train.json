{
  "name": "train",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000
  },
  "augmentations": {
    "wave": [],
    "spectrogram": []
  },
  "arch": {
    "type": "RawNet2",
    "args": {
      "sinc_out_channels": 20,
      "sinc_kernel_size": 129,
      "resblocks_channels": [
        [20, 20],
        [20, 20],
        [20, 128],
        [128, 128],
        [128, 128],
        [128, 128]
      ],
      "gru_size": 1024,
      "gru_num_layers": 3,
      "fc_size": 1024,
      "num_classes": 2
    }
  },
  "data": {
    "train": {
      "batch_size": 32,
      "num_workers": 4,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "part": "train",
            "audio_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA"
          }
        }
      ]
    },
    "test": {
      "batch_size": 32,
      "num_workers": 4,
      "datasets": [
        {
          "type": "ASVSpoof2019Dataset",
          "args": {
            "part": "eval",
            "audio_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA"
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 0.0001,
      "weight_decay": 0.0001
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "args": {}
  },
  "metrics": [],
  "trainer": {
    "epochs": 100,
    "save_dir": "saved/",
    "save_period": 10,
    "verbosity": 2,
    "monitor": "min test_loss",
    "early_stop": 100000000000,
    "visualize": "wandb",
    "wandb_project": "as_project",
    "grad_norm_clip": 10
  }
}
